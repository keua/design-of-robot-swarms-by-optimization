{
  "include_files": [
    "config/kubedaar/automode.json",
    "config/random.json"
  ],
  "execution": {
    "seed_window_size": 10,
    "seed_window_movement": 2,
    "budget": 5000
  },
  "logging": {
    "snapshot_frequency": 100,
    "log_level": "INFO"
  },
  "parallelization": {
    "parallel": 12,
    "mode": "MPI"
  },
  "sls": {
    "SimulatedAnnealing": {
      "temperature": 125.0,
      "cooling_rate": 0.60,
      "temperature_length": 3,
      "acceptance_criterion": "wilcoxon_test"
    }
  }
}