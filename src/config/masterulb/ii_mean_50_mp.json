    "snapshot_frequency": 100,
    "log_level": "INFO"
  },
  "parallelization": {
    "parallel": 5,
    "mode": "multiprocessing"
  },
  "sls": {
    "IterativeImprovement": {
      "acceptance_criterion": "mean"
    }
  }
}